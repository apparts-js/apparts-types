{
  "name": "@apparts/types",
  "version": "6.9.2",
  "description": "Typechecking for requests and in general",
  "main": "index.js",
  "scripts": {
    "test": "jest --watch",
    "test:one": "jest",
    "test:coverage": "jest --coverage",
    "genApiDocs": "node apiDocs/generateHtmlApi.js && node apiDocs/generateMdApi.js",
    "prepublishOnly": "npm run ci && npm ci",
    "ci": "npm ci && npm run ci:prettier && npm run ci:eslint && npm run ci:test",
    "ci:prettier": "CI=true prettier --check .",
    "ci:test": "CI=true npm run test:one",
    "ci:eslint": "eslint . --ext .js,.jsx,.ts,.tsx -c .eslintrc.ci.js"
  },
  "jest": {
    "testTimeout": 5000,
    "clearMocks": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "watchPathIgnorePatterns": [
      "/\\.#.*",
      "/.*~",
      "/#.*#"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:phuhl/apparts-types.git"
  },
  "keywords": [
    "apparts",
    "types",
    "express"
  ],
  "author": "Philipp Uhl",
  "license": "MIT",
  "dependencies": {
    "uuid": "^3.3.2"
  },
  "peerDependencies": {
    "@apparts/config": "^1.0.5",
    "@apparts/error": "^1.1.0",
    "@apparts/model": "^4.0.2"
  },
  "devDependencies": {
    "@apparts/config": "^1.0.5",
    "@apparts/error": "^1.1.0",
    "@apparts/model": "^4.0.2",
    "body-parser": "^1.19.0",
    "eslint": "^8.1.0",
    "express": "^4.17.1",
    "jest": "^27.2.0",
    "prettier": "^2.4.1",
    "supertest": "^4.0.2"
  }
}
