{
  "name": "@apparts/types",
  "version": "6.4.0",
  "description": "Typechecking for requests and in general",
  "main": "index.js",
  "scripts": {
    "test": "jest --watch",
    "testCoverage": "jest --coverage",
    "genApiDocs": "node apiDocs/generateHtmlApi.js && node apiDocs/generateMdApi.js"
  },
  "jest": {
    "testTimeout": 5000,
    "clearMocks": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "watchPathIgnorePatterns": [
      "/\\.#.*",
      "/.*~",
      "/#.*#"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:phuhl/apparts-types.git"
  },
  "keywords": [
    "apparts",
    "types",
    "express"
  ],
  "author": "Philipp Uhl",
  "license": "MIT",
  "dependencies": {
    "uuid": "^3.3.2"
  },
  "peerDependencies": {
    "@apparts/config": "^1.0.5",
    "@apparts/error": "^1.1.0",
    "@apparts/model": "^4.0.2"
  },
  "devDependencies": {
    "@apparts/config": "^1.0.5",
    "@apparts/error": "^1.1.0",
    "@apparts/model": "^4.0.2",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "jest": "^26.0.1",
    "supertest": "^4.0.2"
  }
}
