{
  "name": "@apparts/types",
  "version": "6.15.2",
  "description": "Typechecking for requests and in general",
  "main": "build/index.js",
  "files": [
    "src/apiDocs/reactDocs.js",
    "build/**/*"
  ],
  "types": "build/index.d.ts",
  "scripts": {
    "test": "jest --watch",
    "test:one": "jest",
    "test:coverage": "jest --coverage",
    "genApiDocs": "ts-node src/apiDocs/generateHtmlApi.js && ts-node src/apiDocs/generateMdApi.js && ts-node src/apiDocs/generateReactApi.js",
    "prepublishOnly": "npm run clean && npm ci  && npm run ci && npm run build",
    "ci": "npm ci && npm run ci:prettier && npm run ci:eslint && npm run ci:test",
    "ci:prettier": "CI=true prettier --check .",
    "ci:test": "CI=true npm run test:one",
    "ci:eslint": "eslint . --ext .js,.jsx,.ts,.tsx -c .eslintrc.ci.js",
    "build": "npm run clean && tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "build:types": "tsc --emitDeclarationOnly -p tsconfig.release.json --outDir dts",
    "clean": "rimraf coverage build tmp"
  },
  "jest": {
    "testTimeout": 5000,
    "clearMocks": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "watchPathIgnorePatterns": [
      "/\\.#.*",
      "/.*~",
      "/#.*#"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:phuhl/apparts-types.git"
  },
  "keywords": [
    "apparts",
    "types",
    "express"
  ],
  "author": "Philipp Uhl",
  "license": "MIT",
  "dependencies": {
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "@apparts/config": "^1.0.5",
    "@apparts/error": "^1.1.0",
    "@apparts/model": "^4.0.2"
  },
  "devDependencies": {
    "@apparts/config": "^1.0.5",
    "@apparts/error": "^1.1.0",
    "@apparts/model": "^4.0.2",
    "@tsconfig/node14": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "body-parser": "^1.19.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^26.0.0",
    "express": "^4.17.1",
    "jest": "^27.2.0",
    "prettier": "^2.4.1",
    "supertest": "^4.0.2",
    "ts-jest": "^27.1.3",
    "tsutils": "^3.21.0",
    "typescript": "^4.5.5"
  }
}
